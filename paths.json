{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/renderer/src/*"],
      "@renderer/*": ["src/renderer/*"],
      "@common/*": ["src/common/*"],
      "@main/*": ["src/main/*"],
      "@preload/*": ["src/preload/*"],
      "src/*": ["src/*"]
    }
  }
}